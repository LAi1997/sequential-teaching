      <CodeComponent name="code">
        <Param name="Before Experiment" updates="constant" val="# Begin Experiment&amp;#10;def movePicked(mouse, grabbed):&amp;#10;    # Move piece if we already moving that piece&amp;#10;    if grabbed is not None and mouse.isPressedIn(grabbed):&amp;#10;        (u,v) = mouse.getPos()&amp;#10;        if left &lt;= u and u &lt;= right and bot &lt;= v and v &lt;= top:&amp;#10;            grabbed.pos = (u,v)&amp;#10;        return grabbed&amp;#10;    else:&amp;#10;        # Move newly clicked piece&amp;#10;        for block in blocks:&amp;#10;            if mouse.isPressedIn(block) and grabbed is not block:&amp;#10;                return block" valType="extendedCode"/>
        <Param name="Before JS Experiment" updates="constant" val="function movePicked(mouse, grabbed) {&amp;#10;    var u, v;&amp;#10;    if (((grabbed !== null) &amp;&amp; mouse.isPressedIn(grabbed))) {&amp;#10;        [u, v] = mouse.getPos();&amp;#10;        if (((((left &lt;= u) &amp;&amp; (u &lt;= right)) &amp;&amp; (bot &lt;= v)) &amp;&amp; (v &lt;= top))) {&amp;#10;            grabbed.pos = [u, v];&amp;#10;        }&amp;#10;        return grabbed;&amp;#10;    } else {&amp;#10;        for (var block, _pj_c = 0, _pj_a = blocks, _pj_b = _pj_a.length; (_pj_c &lt; _pj_b); _pj_c += 1) {&amp;#10;            block = _pj_a[_pj_c];&amp;#10;            if ((mouse.isPressedIn(block) &amp;&amp; (grabbed !== block))) {&amp;#10;                return block;&amp;#10;            }&amp;#10;        }&amp;#10;    }&amp;#10;}&amp;#10;" valType="extendedCode"/>
        <Param name="Begin Experiment" updates="constant" val="" valType="extendedCode"/>
        <Param name="Begin JS Experiment" updates="constant" val="" valType="extendedCode"/>
        <Param name="Begin JS Routine" updates="constant" val="blocks = [item1, item2];&amp;#10;movingPiece = null;&amp;#10;[x, y] = sketchboard.pos;&amp;#10;[w, h] = sketchboard.size;&amp;#10;top = (y + (h / 2));&amp;#10;bot = (y - (h / 2));&amp;#10;left = (x - (w / 2));&amp;#10;right = (x + (w / 2));&amp;#10;" valType="extendedCode"/>
        <Param name="Begin Routine" updates="constant" val="# Begin Routine&amp;#10;blocks = [item1,item2]&amp;#10;movingPiece = None&amp;#10;(x, y) = sketchboard.pos&amp;#10;(w,h) = sketchboard.size&amp;#10;top = y+h/2&amp;#10;bot = y-h/2&amp;#10;left = x-w/2&amp;#10;right = x+w/2" valType="extendedCode"/>
        <Param name="Code Type" updates="None" val="Auto-&gt;JS" valType="str"/>
        <Param name="Each Frame" updates="constant" val="# Each Frame&amp;#10;for block in blocks:&amp;#10;    if mouse.isPressedIn(block) and movingPiece is None:&amp;#10;        movingPiece = block&amp;#10;&amp;#10;movingPiece = movePicked(mouse, movingPiece)" valType="extendedCode"/>
        <Param name="Each JS Frame" updates="constant" val="for (var block, _pj_c = 0, _pj_a = blocks, _pj_b = _pj_a.length; (_pj_c &lt; _pj_b); _pj_c += 1) {&amp;#10;    block = _pj_a[_pj_c];&amp;#10;    if ((mouse.isPressedIn(block) &amp;&amp; (movingPiece === null))) {&amp;#10;        movingPiece = block;&amp;#10;    }&amp;#10;}&amp;#10;movingPiece = movePicked(mouse, movingPiece);&amp;#10;" valType="extendedCode"/>
        <Param name="End Experiment" updates="constant" val="" valType="extendedCode"/>
